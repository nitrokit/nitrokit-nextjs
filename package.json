    "private": false,
    "packageManager": "pnpm@10.20.0",
    "scripts": {
        "predev": "./scripts/generate-i18n-declaration.sh",
        "dev": "prisma-import --schemas \"prisma/models/*.prisma\" --output \"prisma/schema.prisma\" --force && prisma generate --no-engine --data-proxy --no-hints && next dev --turbopack",
        "prebuild:prisma": "prisma-import --schemas \"prisma/models/*.prisma\" --output \"prisma/schema.prisma\" --force && prisma generate --no-engine --data-proxy",
        "prebuild": "./scripts/generate-i18n-declaration.sh",
        "build": "prisma-import --schemas \"prisma/models/*.prisma\" --output \"prisma/schema.prisma\" --force && prisma generate --no-engine --data-proxy && next build --turbopack",
        "start": "next start",
        "postinstall": "prisma-import --schemas \"prisma/models/*.prisma\" --output \"prisma/schema.prisma\" --force && prisma generate --no-engine --data-proxy",
        "prepare": "husky",
        "prisma:import": "prisma-import --schemas \"prisma/models/*.prisma\" --output \"prisma/schema.prisma\" --force",
        "prisma:generate": "prisma-import --schemas \"prisma/models/*.prisma\" --output \"prisma/schema.prisma\" --force && prisma generate --no-engine  --data-proxy",
        "prisma:migrate": "prisma-import --schemas \"prisma/models/*.prisma\" --output \"prisma/schema.prisma\" --force && prisma migrate dev",
        "prisma:push": "prisma db push",
        "prisma:studio": "prisma studio",
        "type-check": "tsc --noEmit",
        "lint": "eslint src --no-cache",
        "lint:fix": "eslint src --fix --no-cache",
        "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\" --cache",
        "format:write": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\" --cache",
        "test": "vitest",
        "test:coverage": "vitest run --coverage --reporter=verbose --silent=false",
        "test:ui": "vitest --ui",
        "test:e2e": "playwright test",
        "test:e2e:ui": "playwright test --ui",
        "test:e2e:debug": "playwright test --debug",
        "test:e2e:report": "playwright show-report",
        "doppler:upload": "doppler secrets upload .env"
    },
